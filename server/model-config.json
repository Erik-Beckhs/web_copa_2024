{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "user": {
    "dataSource": "MySQL",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "realmRequired": false,
      "resetPasswordTokenExpirationInSeconds": 3600,
      "maxTTL": 31536000,
      "minTTL": 0
    }
  },
  "AccessToken": {
    "dataSource": "MySQL",
    "public": false
  },
  "ACL": {
    "dataSource": "MySQL",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "MySQL",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "MySQL",
    "public": false
  },
  "hospedaje": {
    "dataSource": "MySQL",
    "public": true
  },
  "atractivo": {
    "dataSource": "MySQL",
    "public": true
  },
  "resena": {
    "dataSource": "MySQL",
    "public": true
  },
  "noticia": {
    "dataSource": "MySQL",
    "public": true
  },
  "multimedia": {
    "dataSource": "MySQL",
    "public": true
  },
  "atractivo_articulo": {
    "dataSource": "MySQL",
    "public": true
  },
  "hosped_servicio": {
    "dataSource": "MySQL",
    "public": true
  },
  "hosped_thabitacion": {
    "dataSource": "MySQL",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true
  },
  "servicio": {
    "dataSource": "MySQL",
    "public": true
  },
  "guia": {
    "dataSource": "MySQL",
    "public": true
  },
  "guia_idioma": {
    "dataSource": "MySQL",
    "public": true
  },
  "guia_tipo": {
    "dataSource": "MySQL",
    "public": true
  },
  "horarios": {
    "dataSource": "MySQL",
    "public": true
  },
  "servicio_pago": {
    "dataSource": "MySQL",
    "public": true
  }
}
